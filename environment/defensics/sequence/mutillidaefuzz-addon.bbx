<?xml version="1.0" encoding="utf-8" standalone="no"?>
<add-on>
    <symbols>
        <rule built-in="tcp-rule-with-proxy" name="http-localhost-81">
            <setting name="timeout" param="input-timeout"/>
            <setting name="outtimeout" param="output-timeout"/>
            <value param="server-host">localhost</value>
            <value param="server-port">81</value>
            <value param="secure">false</value>
            <setting name="proxy" param="proxy-enabled"/>
            <setting name="proxy-host" param="proxy-host"/>
            <setting name="proxy-port" param="proxy-port"/>
            <setting name="proxy-secure-port" param="proxy-secure-port"/>
            <value param="max-input-size">10000</value>
            <value param="half-close">true</value>
        </rule>
